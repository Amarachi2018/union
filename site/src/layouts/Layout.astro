---
import Footer from "#/components/Footer.astro"
import Header from "#/components/Header.astro"
import BaseLayout from "./BaseLayout.astro"

interface Props {
  url?: string
  title?: string
  image?: string
  description?: string
}

const { url, title, image, description } = Astro.props
---

<BaseLayout
  {url}
  {title}
  {image}
  {description}
>
  <Header />
  <slot />
  <Footer />
</BaseLayout>
