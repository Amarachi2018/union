{
  "name": "@unionlabs/sdk",
  "version": "1.7.0",
  "type": "module",
  "license": "MIT",
  "author": "@unionlabs",
  "description": "Union TypeScript SDK 2",
  "repository": {
    "type": "git",
    "url": "https://github.com/unionlabs/union.git",
    "directory": "ts-sdk"
  },
  "exports": {
    ".": {
      "import": "./build/esm/index.js",
      "types": "./build/dts/index.d.ts"
    },
    "./ucs03": {
      "import": "./build/esm/ucs03/index.js",
      "types": "./build/dts/ucs03/index.d.ts"
    },
    "./evm": {
      "import": "./dist/src/evm/index.js",
      "types": "./dist/dts/evm/index.d.ts"
    },
    "./cosmos": {
      "import": "./dist/src/cosmos/index.js",
      "types": "./dist/dts/cosmos/index.d.ts"
    },
    "./evm/abi": {
      "import": "./dist/src/evm/abi/index.js",
      "types": "./dist/dts/evm/abi/index.d.ts"
    },
    "./evm/ucs03": {
      "import": "./dist/src/evm/ucs03/index.js",
      "types": "./dist/dts/evm/ucs03/index.d.ts"
    },
    "./evm/erc20": {
      "import": "./dist/src/evm/erc20.js",
      "types": "./dist/dts/evm/erc20.d.ts"
    },
    "./utils": {
      "import": "./dist/src/utils/index.js",
      "types": "./dist/dts/utils/index.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm build-esm && pnpm build-annotate",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "build-esm": "tsc -b tsconfig.build.json",
    "check": "tsc -b tsconfig.json",
    "check:circular": "dpdm -T src",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "effect": "^3",
    "viem": "^2"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@effect/build-utils": "^0.7.7",
    "@effect/vitest": "~0.19.10",
    "babel-plugin-annotate-pure-calls": "^0.5.0",
    "biome": "^0.3.3",
    "dpdm": "^3.14.0",
    "effect": "^3.13.12",
    "typescript": "^5.8.2",
    "viem": "^2.23.12",
    "vitest": "^3.0.8"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "1.34.0",
    "@cosmjs/cosmwasm-stargate": "^0.33.1",
    "@cosmjs/stargate": "^0.33.1",
    "@scure/base": "1.2.4"
  }
}
