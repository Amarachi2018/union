{
  "name": "@unionlabs/sdk",
  "version": "1.9.9",
  "type": "module",
  "license": "MIT",
  "author": "@unionlabs",
  "homepage": "https://docs.union.build/typescript",
  "description": "Union TypeScript SDK 2",
  "repository": {
    "type": "git",
    "url": "https://github.com/unionlabs/union.git",
    "directory": "ts-sdk"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "directory": "dist",
    "linkDirectory": false
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts",
    "./*": "./src/*.ts",
    "./examples/*": "./examples/*.ts",
    "./internal/*": null
  },
  "scripts": {
    "build": "pnpm build-esm && pnpm build-annotate && pnpm build-cjs && build-utils pack-v3",
    "build-annotate": "babel build/esm --plugins annotate-pure-calls --out-dir build/esm --source-maps",
    "build-cjs": "babel build/esm --plugins @babel/transform-export-namespace-from --plugins @babel/transform-modules-commonjs --out-dir build/cjs --source-maps",
    "build-esm": "tsc -b tsconfig.build.json",
    "check": "tspc -b tsconfig.json",
    "check:circular": "dpdm -T src",
    "check:examples": "tspc -p tsconfig.examples.json",
    "codegen": "build-utils prepare-v3",
    "prepare": "ts-patch install -s",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "peerDependencies": {
    "@effect/platform": "^0.84",
    "@effect/experimental": "^0.48",
    "effect": "^3.16",
    "viem": "^2"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.1",
    "@babel/plugin-transform-export-namespace-from": "^7.27.1",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@cosmjs/math": "^0.33.1",
    "@effect/build-utils": "^0.8.3",
    "@effect/experimental": "0.48.6",
    "@effect/platform": "0.84.6",
    "@types/node": "^22.13.1",
    "babel-plugin-annotate-pure-calls": "^0.5.0",
    "dpdm": "^3.14.0",
    "effect": "3.16.3",
    "madge": "^8.0.0",
    "ts-patch": "^3.3.0",
    "vitest": "^3.0.5"
  },
  "dependencies": {
    "@aptos-labs/ts-sdk": "1.34.0",
    "@cosmjs/cosmwasm-stargate": "^0.33.1",
    "@cosmjs/proto-signing": "^0.33.0",
    "@cosmjs/stargate": "^0.33.1",
    "@cosmjs/tendermint-rpc": "^0.33.1",
    "@effect/typeclass": "^0.35.5",
    "@mysten/sui": "^1.30.1",
    "@pythnetwork/hermes-client": "^2.0.0",
    "@redstone-finance/sdk": "^0.8.0",
    "@scure/base": "1.2.4",
    "crc": "^4.3.2",
    "gql.tada": "^1.8.10",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2"
  }
}
